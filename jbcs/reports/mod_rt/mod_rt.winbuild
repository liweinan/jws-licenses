# Build mod_rt
#

install -m 755 -d "$RPM_BUILD_ROOT$_confdir/httpd/conf.d"

EXTLIBPATH="$RPM_ROOT_DIR/$_lib"
EXTINCPATH="$RPM_ROOT_DIR/include"
export EXTLIBPATH
export EXTINCPATH
$__make PREFIX=$RPM_BUILD_ROOT$_prefix install
test $? -ne 0 && exit 1
install -m 644 $_sourcedir/mod_rt.conf "$RPM_BUILD_ROOT$_confdir/httpd/conf.d/mod_rt.conf.sample"

files="$_libdir $_confdir"
